#include<iostream>
using namespace std;
int main()
{
    int a[10][10],b[10][10],r1,r2,c1,c2,i,j;
    cout<<"Enter the no. of row of 1st array : ";
    cin>>r1;
    cout<<"Enter the no. of column of 1st array : ";
    cin>>c1;
    cout<<"Enter the no. of row of 2nd array : ";
    cin>>r2;
    cout<<"Enter the no. of column of 2nd array : ";
    cin>>c2;
    cout<<"Enter the elements in 1st array : ";
    for(i=0;i<r1;i++)
    {
        for(j=0;j<c1;j++)
        {
            cin>>a[i][j];
        }
    }
    cout<<"Enter the elements in 2nd array : ";
    for(i=0;i<r2;i++)
    {
        for(j=0;j<c2;j++)
        {
            cin>>b[i][j];
        }
    }
    cout<<"Sum of both 2-D array : ";
    for(i=0;i<r1;i++)
    {
        for(j=0;j<c1;j++)
        {
            a[i][j]=a[i][j]+b[i][j];
        }
    }
     for(i=0;i<r1;i++)
    {
        for(j=0;j<c1;j++)
        {
            cout<<a[i][j]<<" ";
        }
    }
}
